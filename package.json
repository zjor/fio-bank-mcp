{
  "name": "fio-bank-mcp",
  "version": "1.0.0",
  "description": "MCP server for FIO Bank API (Czech Republic)",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "fio-bank-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test:client": "tsx test-fio-client.ts",
    "inspect": "npx @modelcontextprotocol/inspector node dist/index.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "prepare": "pnpm run build"
  },
  "keywords": [
    "mcp",
    "fio",
    "bank",
    "api",
    "czech",
    "banking"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.0",
    "eslint": "^9.17.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.15.1"
}
